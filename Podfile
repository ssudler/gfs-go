# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'GFS GO' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for GFS GO
  pod 'GoogleSignIn'
  pod 'Firebase/Core'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  target 'GFS GOTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'GFS GOUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
